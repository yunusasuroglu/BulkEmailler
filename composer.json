{
    "name": "yunusasuroglu/bulk-mailler",
    "version": "1.0.0",
    "description": "Bulk email sending package for Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Yunus Emre Aşuroğlu",
            "email": "palitorch24@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "YunusAsuroglu\\BulkEmailler\\": "src/",
            "YunusAsuroglu\\BulkEmailler\\Facades\\": "src/Facades/"
        }
    },
    
    "extra": {
        "laravel": {
            "providers": [  
                "YunusAsuroglu\\BulkEmailler\\BulkEmailerServiceProvider"  
            ],
            "aliases": {
                "BulkEmailer": "YunusAsuroglu\\BulkEmailler\\Facades\\BulkEmailer"
            }
        },
        "repositories": [
            {
                "type": "vcs",
                "url": "https://github.com/yunusasuroglu/bulk-mailler"
            }
        ]
        
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}


{
    "name": "yunusasuroglu/bulk-emailler",
    "description": "Laravel için toplu e-posta gönderimi paketi",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Yunus Emre Aşuroğlu",
            "email": "palitorch24@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "YunusAsuroglu\\BulkEmailler\\": "src/",
            "YunusAsuroglu\\BulkEmailler\\Facades\\": "src/Facades/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [  
                "YunusAsuroglu\\BulkEmailler\\BulkEmailerServiceProvider"  
            ],
            "aliases": {
                "BulkEmailer": "YunusAsuroglu\\BulkEmailler\\Facades\\BulkEmailer"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
